{
    "light-mode": [
        {
            "id": "route-outline",
            "type": "line",
            "source": "internal-api",
            "source-layer": "routes",
            "minzoom": 8,
            "layout": {"line-cap": "round", "line-join": "round"},
            "paint": {
                "line-width": [
                    "interpolate",
                    ["linear"],
                    ["zoom"],
                    11,
                    6,
                    16,
                    30,
                    22,
                    86
                ],
                "line-color": [
                    "match",
                    ["get", "route_color"],
                    ["red"],
                    "hsl(0, 88%, 75%)",
                    ["orange"],
                    "hsl(33, 94%, 69%)",
                    ["yellow"],
                    "hsl(50, 93%, 73%)",
                    ["green"],
                    "hsl(89, 76%, 65%)",
                    ["blue"],
                    "hsl(226, 100%, 77%)",
                    ["purple"],
                    "hsl(275, 81%, 76%)",
                    ["pink"],
                    "hsl(331, 75%, 75%)",
                    ["brown"],
                    "hsl(23, 39%, 69%)",
                    ["get", "route_color"]
                ],
                "line-opacity": [
                    "interpolate",
                    ["linear"],
                    ["zoom"],
                    8,
                    0,
                    9.5,
                    0.25
                ]
            }
        },
        {
            "id": "route-primary",
            "type": "line",
            "source": "internal-api",
            "source-layer": "routes",
            "minzoom": 8,
            "layout": {"line-cap": "round", "line-join": "round"},
            "paint": {
                "line-width": [
                    "interpolate",
                    ["linear"],
                    ["zoom"],
                    11,
                    4,
                    16,
                    16,
                    22,
                    62
                ],
                "line-color": [
                    "match",
                    ["get", "route_color"],
                    ["red"],
                    "hsl(0, 88%, 75%)",
                    ["orange"],
                    "hsl(33, 94%, 69%)",
                    ["yellow"],
                    "hsl(50, 93%, 73%)",
                    ["green"],
                    "hsl(89, 76%, 65%)",
                    ["blue"],
                    "hsl(226, 100%, 77%)",
                    ["purple"],
                    "hsl(275, 81%, 76%)",
                    ["pink"],
                    "hsl(331, 75%, 75%)",
                    ["brown"],
                    "hsl(23, 39%, 69%)",
                    ["get", "route_color"]
                ],
                "line-opacity": [
                    "interpolate",
                    ["linear"],
                    ["zoom"],
                    8.5,
                    0,
                    9.5,
                    1
                ]
            }
        },
        {
            "id": "stops-icon",
            "type": "circle",
            "source": "internal-api",
            "source-layer": "stops",
            "minzoom": 8,
            "maxzoom": 14.5,
            "paint": {
                "circle-translate": [0, 0],
                "circle-radius": [
                    "interpolate",
                    ["linear"],
                    ["zoom"],
                    11.5,
                    3,
                    13,
                    4
                ],
                "circle-opacity": [
                    "interpolate",
                    ["linear"],
                    ["zoom"],
                    8,
                    ["case", ["==", ["get", "is_major"], false], 0, 0],
                    9,
                    ["case", ["==", ["get", "is_major"], false], 0, 1],
                    13,
                    ["case", ["==", ["get", "is_major"], false], 0, 1],
                    13.5,
                    ["case", ["==", ["get", "is_major"], false], 1, 1]
                ],
                "circle-stroke-width": [
                    "interpolate",
                    ["linear"],
                    ["zoom"],
                    11,
                    1,
                    14,
                    3
                ],
                "circle-stroke-opacity": [
                    "interpolate",
                    ["linear"],
                    ["zoom"],
                    8,
                    ["case", ["==", ["get", "is_major"], false], 0, 0],
                    9,
                    ["case", ["==", ["get", "is_major"], false], 0, 1],
                    13,
                    ["case", ["==", ["get", "is_major"], false], 0, 1],
                    13.5,
                    ["case", ["==", ["get", "is_major"], false], 1, 1]
                ],
                "circle-color": [
                    "interpolate",
                    ["linear"],
                    ["zoom"],
                    11,
                    "hsl(0, 0%, 85%)",
                    14,
                    "hsl(0, 0%, 25%)"
                ],
                "circle-stroke-color": "#222222"
            }
        },
        {
            "id": "stops-label",
            "type": "symbol",
            "source": "internal-api",
            "source-layer": "stops",
            "minzoom": 11,
            "layout": {
                "text-optional": true,
                "text-size": 17,
                "icon-image": [
                    "step",
                    ["zoom"],
                    "",
                    14,
                    ["concat", ["get", "wheelchair_boarding"], "-dark"]
                ],
                "icon-size": 0.4,
                "text-font": [
                    "Akzidenz-Grotesk Std Med Regular",
                    "Arial Unicode MS Regular"
                ],
                "icon-allow-overlap": true,
                "text-padding": 6,
                "text-offset": [
                    "interpolate",
                    ["linear"],
                    ["zoom"],
                    11,
                    ["literal", [0, -0.35]],
                    16,
                    ["literal", [0, -0.85]]
                ],
                "icon-optional": true,
                "text-anchor": "bottom",
                "text-field": ["to-string", ["get", "stop_name"]],
                "text-letter-spacing": -0.01,
                "icon-padding": 5,
                "text-max-width": 8
            },
            "paint": {
                "text-halo-color": "hsla(0, 0%, 96%, 0.85)",
                "text-color": "hsl(0, 0%, 25%)",
                "icon-opacity": [
                    "interpolate",
                    ["linear"],
                    ["zoom"],
                    14,
                    0,
                    14.5,
                    1
                ],
                "text-opacity": [
                    "interpolate",
                    ["linear"],
                    ["zoom"],
                    14,
                    ["case", ["==", ["get", "is_major"], false], 0, 1],
                    14.5,
                    ["case", ["==", ["get", "is_major"], false], 1, 1]
                ],
                "text-halo-width": 4
            }
        }
    ],
